{
  "version": 2,
  "builds": [
    { "src": "api/crossWord.py", "use": "@vercel/python" },
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/data", "dest": "/api/data" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
